{
  "name": "example-nextjs",
  "version": "0.1.15",
  "private": true,
  "scripts": {
    "build": "pnpm run build:all",
    "build:all": "pnpm run build:powersync-common && pnpm run build:powersync-web && pnpm run build:next",
    "build:next": "next build",
    "build:powersync-common": "cd local_modules/@powersync/common && pnpm install && pnpm run build",
    "build:powersync-web": "cd local_modules/@powersync/web && pnpm install && pnpm run build",
    "clean": "rm -rf .next",
    "watch": "next dev",
    "watch:powersync-common": "cd local_modules/@powersync/common && pnpm install && pnpm run watch",
    "watch:powersync-web": "cd local_modules/@powersync/web && pnpm install && pnpm run watch",
    "watch:all": "concurrently \"pnpm run watch:powersync-common\" \"pnpm run watch:powersync-web\" \"pnpm run watch\"",
    "start": "next start",
    "lint": "next lint",
    "test:build": "pnpm build"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.1.0",
    "@journeyapps/wa-sqlite": "^1.2.0",
    "@lexical/react": "^0.20.0",
    "@mui/icons-material": "^6.1.9",
    "@mui/material": "^6.1.9",
    "@powersync/common": "file:local_modules/@powersync/common",
    "@powersync/react": "^1.5.1",
    "@powersync/web": "file:local_modules/@powersync/web",
    "@supabase/supabase-js": "^2.46.2",
    "@types/react-virtualized": "^9.22.0",
    "@types/uuid": "^10.0.0",
    "humanize-duration": "^3.32.1",
    "js-logger": "^1.6.1",
    "lato-font": "^3.0.0",
    "lexical": "^0.20.0",
    "lucide-react": "^0.462.0",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "next": "15.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-virtualized": "^9.22.5",
    "uuid": "^11.0.3",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.15.0",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.4.49",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.5.3",
    "async-mutex": "^0.4.0",
    "event-iterator": "^2.0.0",
    "bson": "^6.10.3",
    "can-ndjson-stream": "^1.0.2",
    "cross-fetch": "^4.0.0",
    "rsocket-core": "1.0.0-alpha.3",
    "rsocket-websocket-client": "1.0.0-alpha.3"
  }
}
